#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    if (n <= 0 || n > 1000000000){
        printf("ERROR");
        return 0;
    }
    
    int sum = 0;

    while (n > 0){
        int cur = n%10;
        if (cur%2==1){
            sum += cur;
        }
        n = n/10;
    }
    printf("%d",sum);
}
