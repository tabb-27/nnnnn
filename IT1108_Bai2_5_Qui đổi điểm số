#include <stdio.h>

int main()
{
    float x;
    scanf("%f",&x);
    if (x < 0.0 || x > 10.0){
        printf("ERROR");
        return 0;
    }
    else if (x < 4.0) {
        printf("F");
    }
    else if (x < 5.0) {
        printf("D");
    }
    else if (x < 5.5) {
        printf("D+");
    }
    else if (x < 6.5) {
        printf("C");
    }
    else if (x < 7.0) {
        printf("C+");
    }
    else if (x < 8.0) {
        printf("B");
    }
    else if (x < 8.5) {
        printf("B+");
    }
    else if (x < 9.5) {
        printf("A");
    }
    else {  // 9.5 <= x <= 10
        printf("A+");
    }

    return 0;
}
